+	public File getResFolder() {
+		File resFolder = new File(getDataFolder(), "res");
+		if(!resFolder.exists())
+			resFolder.mkdir();
+		return resFolder;
+	}
+
+	public HaxorConf getConf() {
+		if(conf == null) conf = new HaxorConf(this);
+		return conf;
+	}
+}
